{
    "name": "skom/skom-sitepackage",
    "type": "typo3-cms-extension",
    "description": "Basic Sitepackage by SKom",
    "homepage": "https://www.skom.de",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "version": "1.1.0",
    "require": {
        "typo3/cms-core": "^8.7 || ^9.5",
        "typo3/cms-rte-ckeditor": "^8.7 || ^9.5",
        "bk2k/bootstrap-package": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Skom\\SkomSitepackage\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
          "extension-key": "skom-sitepackage"
        }
    },
    "support": {
        "issues": "https://github.com/Starraider/skom_sitepackage/issues"
    }
}
